<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="simtek.gameplanner" >

    <application
        android:name=".android.CustomApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".android.Intro_Activity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".android.Activity_launcher"
            android:label="@string/title_activity_activity_launcher" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".android.Gameinfo_Activity"
            android:label="@string/title_activity_activity_02" >
        </activity>
        <activity
            android:name=".android.CreateGame_Activity"
            android:label="@string/title_activity_activity_03" >
        </activity>
        <activity
            android:name=".android.Arenapicker"
            android:label="@string/title_activity_arenapicker" >
        </activity>
        <activity
            android:name=".android.Officialspicker_activity"
            android:label="@string/title_activity_officials_picker" >
        </activity>
        <activity
            android:name=".android.Refinfo_Activity"
            android:label="@string/title_activity_refinfo_" >
        </activity>
    </application>

</manifest>
